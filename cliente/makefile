###########################################################################################################################################################
# #########################################################

# Compila las clases

# #########################################################

compile:
	javac *.java
	
	
# #########################################################

# Ejecuta el cliente

# #########################################################

execute:
	java -Djava.security.policy=cliente.permisos -Djava.net.preferIPv4Stack=true juegos 192.168.254.139 54321

# #########################################################

# Limpia los .class

# #########################################################

clean:
	rm *.class
